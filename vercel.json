{
        "version": 2,
        "regions": ["hnd1"],
      
        "functions": {
          "api/index.php": {
            "runtime": "vercel-php@0.6.2"
          }
        },

        "routes": [
          { "handle": "filesystem" },
          { "src": "/(.*)", "dest": "/api/index.php" }
        ], 

        "outputDirectory": "public/build",
      
        "buildCommand": "corepack enable && corepack prepare pnpm@latest --activate && pnpm install --no-frozen-lockfile && pnpm approve-builds && pnpm run build && php artisan config:clear && php artisan view:clear && php artisan route:clear",
    "env": {
        "APP_NAME": "Laravel Vercel",
        "APP_KEY": "base64:03+keM3YHpsXYkW0m4/t/ojMakU5PSLcqFjSktFI2LY=",
        "APP_DEBUG": "true",
        "APP_ENV": "production",
        "APP_URL": "",
        "SESSION_SECURE_COOKIE": "true",
        "SANCTUM_STATEFUL_DOMAINS": ".vercel.app,localhost,127.0.0.1",
        "SESSION_DOMAIN": ".vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",

{
        "version": 2,
        "regions": ["hnd1"],
      
        "functions": {
          "api/index.php": {
            "runtime": "vercel-php@0.6.2"
          }
        },
      
        "installCommand": "corepack enable && corepack prepare pnpm@latest --activate && pnpm install && pnpm run build",
      
        "headers": [
          {
            "source": "/build/(.*)",
            "headers": [
              { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
            ]
          },
          {
            "source": "/(.*)",
            "headers": [
              { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" }
            ]
          }
        ],
      
        "rewrites": [
          { "source": "/build/(.*)", "destination": "/public/build/$1" },
          { "source": "/storage/(.*)", "destination": "/public/storage/$1" },
          { "source": "/(.*)", "destination": "/api/index.php" }
        ],
    "env": {
        "APP_NAME": "Laravel Vercel",
        "APP_KEY": "base64:03+keM3YHpsXYkW0m4/t/ojMakU5PSLcqFjSktFI2LY=",
        "APP_DEBUG": "true",
        "APP_ENV": "production",
        "APP_URL": "",
        "SESSION_SECURE_COOKIE": "true",
        "SANCTUM_STATEFUL_DOMAINS": ".vercel.app,localhost,127.0.0.1",
        "SESSION_DOMAIN": ".vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp",
        "STORAGE_PATH": "/tmp/storage",
        "DB_CONNECTION":"pgsql",
        "DB_PORT":"5432",
        "DB_URL":"postgresql://neondb_owner:npg_tWXQ35lGMoRI@ep-little-pond-ahvsgrul-pooler.c-3.us-east-1.aws.neon.tech/neondb?sslmode=require&channel_binding=require",
        "NODE_VERSION":"22.11.0"
    }
}
